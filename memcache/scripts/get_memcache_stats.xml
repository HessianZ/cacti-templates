<interface>
	<name>Get Memcache Statistics</name>
	<description>probes the memcache for which ports have  running 
         instances, then runs stats queries against them</description>
	<script_path>sh |path_cacti|/scripts/get_memcache_stats.sh</script_path>
        <arg_prepend>|host_hostname|</arg_prepend>
	<arg_index>index</arg_index>
	<arg_query>query</arg_query>
        <arg_get>get</arg_get>
        <output_delimeter>:</output_delimeter>
        <fields>
                <ports>
                        <name>Ports</name>
                        <direction>input</direction>                        
			<query_name>ports</query_name>
                </ports>
		<total_items>
			<name>total_items</name>
                        <direction>output</direction>                        
			<query_name>total_items</query_name>
		</total_items>
		<get_hits>
			<name>get_hits</name>
                        <direction>output</direction>                        
			<query_name>get_hits</query_name>
		</get_hits>
		<uptime>
			<name>uptime</name>
                        <direction>output</direction>                        
			<query_name>uptime</query_name>
		</uptime>
		<cmd_get>
			<name>cmd_get</name>
                        <direction>output</direction>                        
			<query_name>cmd_get</query_name>
		</cmd_get>
		<time>
			<name>time</name>
                        <direction>output</direction>                        
			<query_name>time</query_name>
		</time>
		<bytes>
			<name>bytes</name>
                        <direction>output</direction>                        
			<query_name>bytes</query_name>
		</bytes>
		<curr_connections>
			<name>curr_connections</name>
                        <direction>output</direction>                        
			<query_name>curr_connections</query_name>
		</curr_connections>
		<connection_structures>
			<name>connection_structures</name>
                        <direction>output</direction>                        
			<query_name>connection_structures</query_name>
		</connection_structures>
		<bytes_written>
			<name>bytes_written</name>
                        <direction>output</direction>                        
			<query_name>bytes_written</query_name>
		</bytes_written>
		<limit_maxbytes>
			<name>limit_maxbytes</name>
                        <direction>output</direction>                        
			<query_name>limit_maxbytes</query_name>
		</limit_maxbytes>
		<cmd_set>
			<name>cmd_set</name>
                        <direction>output</direction>                        
			<query_name>cmd_set</query_name>
		</cmd_set>
		<curr_items>
			<name>curr_items</name>
                        <direction>output</direction>                        
			<query_name>curr_items</query_name>
		</curr_items>
		<rusage_user>
			<name>rusage_user</name>
                        <direction>output</direction>                        
			<query_name>rusage_user</query_name>
		</rusage_user>
		<get_misses>
			<name>get_misses</name>
                        <direction>output</direction>                        
			<query_name>get_misses</query_name>
		</get_misses>
		<evictions>
			<name>evictions</name>
                        <direction>output</direction>                        
			<query_name>evictions</query_name>
		</evictions>
		<rusage_system>
			<name>rusage_system</name>
                        <direction>output</direction>                        
			<query_name>rusage_system</query_name>
		</rusage_system>
		<bytes_read>
			<name>bytes_read</name>
                        <direction>output</direction>                        
			<query_name>bytes_read</query_name>
		</bytes_read>
		<total_connections>
			<name>total_connections</name>
                        <direction>output</direction>                        
			<query_name>total_connections</query_name>
		</total_connections>
        </fields>
</interface>
